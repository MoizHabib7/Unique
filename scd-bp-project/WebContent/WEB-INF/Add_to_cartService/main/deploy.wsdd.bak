<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from Add_to_cartService WSDL service -->

  <service name="Add_to_cart" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://main"/>
      <parameter name="wsdlServiceElement" value="Add_to_cartService"/>
      <parameter name="schemaQualified" value="http://main"/>
      <parameter name="wsdlServicePort" value="Add_to_cart"/>
      <parameter name="className" value="main.Add_to_cartSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Add_to_cart"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getcartdetails" qname="operNS:getcartdetails" xmlns:operNS="http://main" soapAction="" >
        <parameter qname="pns:userid" xmlns:pns="http://main" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getcartdetails"/>

  </service>
</deployment>
